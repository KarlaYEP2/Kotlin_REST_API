spring:
    datasource:
        driver-class-name: "org.h2.Driver"
        url: "jdbc:h2:file:~/file_name"
        username: "username"
        password: "password"
    jpa:
        hibernate:
            ddl-auto: update
        show-sql: true
        generate-ddl: false
        properties:
            hibernate:
                dialect: org.hibernate.dialect.H2Dialect